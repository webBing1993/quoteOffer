@import "/utils/common.wxss";
@import "/pages/tabbar/index.wxss";
page {
  padding-bottom: 200rpx;
}

.content {
  position: fixed;
  width: 100vw;
  height: calc(100vh - 170rpx);
  left: 0;
  top: 0;
  overflow-y: scroll
}

/* title  */
.title {
  height: 120rpx;
  line-height: 120rpx;
  padding: 0 30rpx;
}

.title .fl {
  color: #333;
  font-size: 36rpx;
}

.title .fr {
  color: #C71B1B;
  font-size: 24rpx;
}

checkbox-group>label,radio-group label navigator {
  padding-left: 80rpx;
  display: block;
  /* height: 96rpx; */
  line-height: 96rpx;
  margin-bottom: 20rpx;
  background-color: #fff;
  color: #333;
  font-size: 32rpx;
  position: relative;
}

checkbox-group>label>text,checkbox-group>label>navigator>text,radio-group>label>navigator>text {
  width: 30rpx;
  height: 30rpx;
  border: 2rpx solid #c2c2c2;
  border-radius: 100%;
  display: inline-block;
  position: absolute;
  left: 30rpx;
  top: 33rpx;
}

checkbox-group>label>image,checkbox-group>label>navigator>image,radio-group>label>navigator>image {
  display: inline-block;
  width: 32rpx;
  height: 32rpx;
  position: absolute;
  left: 30rpx;
  top: 33rpx;
}

checkbox-group checkbox-group>label>navigator>image,checkbox-group checkbox-group>label>navigator>text {
  left: 0;
}

checkbox-group>label>.fr,checkbox-gtoup>label>navigator>.fr,radio-group>label>.fr {
  display: inline-block;
  padding: 42rpx 31rpx;
  width: 22rpx;
  height: 12rpx;
}

image {
  width: 100%;
  height: 100%;
  display: block
}

checkbox-group>label>.fr>image.up {
  transform: rotate(180deg)
}

checkbox-group checkbox-group {
  border-top: 1px solid #f0f0f0;
  margin-left: -50rpx;
}

checkbox-group checkbox-group>label  {
  padding-left: 50rpx;
  color: #666;
  font-size: 28rpx;
}

checkbox-group checkbox-group>label {
  padding-left: 0;
  margin-bottom: 0
}

checkbox-group checkbox-group>label>navigator {
  display: block;
  width: calc(100vw - 90rpx);
  height: 100%;
  margin-left: -30rpx;
  padding-left: 90rpx;

}

checkbox-group checkbox-group>label .fr {
  padding: 0 31rpx;
  width: auto;
  height: 96rpx;
  font-size: 18rpx;
  color: #C71B1B;
}

checkbox-group checkbox-group>label .fr text {
  font-size: 28rpx;
}

checkbox-group.changeColor>label {
  padding-left: 0;
}

checkbox-group.changeColor>label navigator {
  display: block;
  padding-left: 80rpx;
}

checkbox-group.changeColor>label .fire,checkbox-group.changeColor>label .total_  {
  padding-left: 130rpx;
}


checkbox-group.changeColor>label .remarkTotal {
  padding-left: 50rpx;
}

.total_,.fire,.remarkTotal {
  border-top: 1px solid #f0f0f0;
  height: 100rpx;
  line-height: 100rpx;
  margin-left: -50rpx;
  padding-left: 50rpx;
}

.total_ .fl,.fire .fl ,.remarkTotal .fl {
  color: #666;
  font-size: 24rpx;
}

.total_ .fr,.remarkTotal .fr {
  color: #666;
  font-size: 24rpx;
}

.total_ .fr text:first-of-type,.remarkTotal .fr text:first-of-type {
  font-size: 18rpx;
  color: #C71B1B;
}

.total_ .fr text:last-of-type,.remarkTotal .fr text:last-of-type {
  font-size: 36rpx;
  color: #C71B1B;
  padding: 0 31rpx 0 0;
}

.right {
  float: right;
  font-size: 24rpx;
  color: #666;
  margin-right: 30rpx;
}

.oldPrice {
  color: #999;
  font-size: 14rpx;
  margin-right: 15rpx;
}

.oldPrice text {
  font-size: 24rpx;
  text-decoration: line-through
}

.newPrice {
  color: #C71B1B;
  font-size: 18rpx;
}

.newPrice text {
  font-size: 36rpx;
}

.fire>.fr {
  margin-right: 30rpx;
  padding-top: 25rpx;
}

.fire .fr text {
  width: 50rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  font-size: 18rpx;
  border: 1px solid #C71B1B;
  border-radius: 5rpx;
  display: inline-block
}

.fire .fr text:nth-last-of-type(n+2) {
  margin-left: -1px;
}

.fire .fr text:first-of-type,.fire .fr text:last-of-type {
  font-size: 24rpx;
}

.fire text.noMinus {
  background-color: #f0f0f0;
}

checkbox-group>label.checkbox4 {
  margin-bottom: 0;
}

.line {
  width: calc(100% + 50rpx);
  background-color: #f0f0f0;
  height: 1px;
  margin-left: -50rpx;
}

checkbox-group>label.checkbox4:last-of-type .line {
  height: 0
}

/* 客户信息  */
.lists {
  background-color: #fff;
  padding-left: 30rpx;
}

.lists .li {
  height: 80rpx;
  line-height: 80rpx;
  width: 100%;
  border-bottom: 1px solid #f0f0f0;
}

.lists .li:last-of-type {
  border-bottom: 0;
}

.lists .li .fl {
  color: #666;
  font-size: 28rpx;
}

.lists .li .fr {
  color: #666;
  font-size: 28rpx;
  margin-right: 30rpx;
  text-align: right;
  height: 80rpx;
  line-height: 80rpx;
}

.lists .li .fr.placeholder,.lists textarea.placeholder {
  color: #999;
  font-size: 28rpx;
}

.lists textarea {
  min-height: 240rpx;
  color: #666;
  font-size: 28rpx;
  width: 100%;
}

.lists.remark {
  padding-left: 0
}

.lists.remark navigator {
  margin-bottom: 30rpx;
  padding: 27rpx 30rpx;
  color: #666;
  font-size: 28rpx;
  min-height: 240rpx;
}

.lists.remark view {
  min-height: 240rpx;
  color: #666;
  font-size: 28rpx;
  width: 100%;
}

/* btn  */
.btn {
  padding: 0 30rpx;
  margin-bottom: 36rpx;
}

.btn .bulid,.btn view {
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 36rpx;
  background-color: #C71B1B;
  color: #fff;
  border-radius: 5rpx;
}

.btn view {
  background-color: #E07778;
  opacity: .6
}

/* total  */
.total {
  height: 80rpx;
  line-height: 80rpx;
  padding: 0 30rpx;
  background-color: #fff;
  position: fixed;
  bottom: 88rpx;
  left: 0;
  width: calc(100vw - 60rpx);
  z-index: 10;
  border-top:1px solid #f0f0f0;
}

.total .fl {
  color: #999;
  font-size: 24rpx;
}

.total .fr {
  font-size: 18rpx;
  color: #C71B1B;
}

.total .fr text {
  font-size: 36rpx;
}

/* info  */
.info {
  position: fixed;
  z-index: 12;
  left: 0;
  top: 0;
  width: 100vw;
  height: 100vh;
}

.info .shadow,.textareaModal .shadow {
  position: fixed;
  z-index: 15;
  width: 100vw;
  height: 100vh;
  left: 0;
  top: 0;
  background-color: #000;
  opacity: .4
}

.info .infoBox {
  position: fixed;
  width: 68%;
  left: 50%;
  transform: translateY(-50%);
  padding: 83rpx 31rpx;
  margin-left: -34%;
  top: 50%;
  background-color: #fff;
  border-radius: 20rpx;
  text-align: center;
  z-index: 18
}

.info .infoBox text {
  color: #666;
  font-size: 28rpx;
  margin-bottom: 61rpx;
  display: block;
}

.info .infoBox button {
  width: 252rpx;
  height: 60rpx;
  line-height: 60rpx;
  background-color: #C71B1B;
  border-radius: 30rpx;
  text-align: center;
  font-size: 30rpx;
  color: #fff;
}

/* caseImg  */
.caseImg .shadow {
  position: fixed;
  width: 100vw;
  height: 100vh;
  left: 0;
  top: 0;
  z-index: 20; 
  background-color: #000;
  opacity: .6 
}

.caseImg swiper {
  position: fixed;
  left: 50%;
  top: 20%;
  transform: translateX(-50%);
  z-index: 25;
  background-color: #FFFFFF;
  border-radius: 20rpx;
  padding: 70rpx 88rpx 42rpx;
  width: calc(550rpx - 176rpx);
  height: 488rpx
}

.caseImg swiper image {
  display: block;
  width: 375rpx;
  height: 328rpx;
  margin-bottom: 63rpx;
}

.caseImg swiper view {
  color: #666;
  font-size: 28rpx;
  margin-bottom: 60rpx;
  text-align: center
}

.caseImg .close {
  position: fixed;
  z-index: 25;
  left: 50%;
  transform: translateX(-50%);
  bottom: 20%;
  width: 80rpx;
  height: 80rpx;
}

.caseImg .close image {
  display: block;
  width: 100%;
  height: 100%
}

.shadow1 {
  position: fixed;
  width: 100vw;
  height: 100vh;
  left: 0;
  top: 0;
  background-color: #f1f1f1;
  z-index: 30
}

.shadow1.shadow_ {
  animation: dialogSlipToUp 800ms ease;
}

 @keyframes dialogSlipToUp  {
    0%{
        opacity: 1;
    }
    50% {
      opacity: .5;
    }
    100%{
        opacity: 0;
    }
}

.textareaModal>view:last-of-type {
  position: fixed;
  left: 50%;
  top: 50%;
  z-index: 30;
  border-radius: 20rpx;
  background-color: #fff;
  transform: translate(-50%, -80%);
}

.textareaModal>view:last-of-type>view {
  position: relative;
  padding-bottom: 80rpx;
}

.modalBtn {
  position: fixed;
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  bottom: 0;
  left: 0;
  border-top: 1px solid #f0f0f0;
}

.modalBtn navigator {
  width: 49.5%;
  text-align: center;
  display: block;
  font-size: 36rpx;
  position: relative;
}

.modalBtn navigator:first-of-type {
  border-left: 20rpx;
  color: #666;
}

.modalBtn navigator:last-of-type {
  border-right: 20rpx;
  color: #C71B1B;
  font-weight: bold
}

.modalBtn navigator:first-of-type:after {
  content: '';
  position: absolute;
  right: 0;
  top: 0;
  width: 1px;
  height: 100%;
  background-color: #f0f0f0;
}

.textareaModal textarea {
  height: 248rpx;
  color: #999;
  font-size: 28rpx;
  padding: 50rpx;
  width: calc(100vw - 260rpx);
}

.textareaModal textarea.placeholder {
  color: #999;
  font-size: 28rpx;
}
